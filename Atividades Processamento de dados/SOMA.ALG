Algoritmo "soma"

Var
   valor1, valor2, resultado: inteiro


Inicio

      escreval("Escreva o primeiro valor: ")
      leia(valor1)
      escreval("Escreva o segundo valor: ")
      leia(valor2)
      
      resultado <- valor1 + valor2
      
      escreval("O resultado é: ", resultado)


Fimalgoritmo